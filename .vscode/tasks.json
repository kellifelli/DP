{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "run",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run",
                "${workspaceFolder}/DP.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [ 
                "publish",
                "--configuration",
                "Release",
                "--runtime",
                "win10-x64"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}